<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.health.mapper.DietMapper">
  <resultMap id="BaseResultMap" type="com.health.entity.Diet">
    <result column="id" jdbcType="INTEGER" property="id" />
    <result column="total" jdbcType="DOUBLE" property="total" />
    <result column="breakfast" jdbcType="DOUBLE" property="breakfast" />
    <result column="lunch" jdbcType="DOUBLE" property="lunch" />
    <result column="dinner" jdbcType="DOUBLE" property="dinner" />
    <result column="sport" jdbcType="DOUBLE" property="sport" />
    <result column="Abreakfast" jdbcType="DOUBLE" property="abreakfast" />
    <result column="Adinner" jdbcType="DOUBLE" property="adinner" />
    <result column="Alunch" jdbcType="DOUBLE" property="alunch" />
    <result column="Asport" jdbcType="DOUBLE" property="asport" />
  </resultMap>
  <select id="getDiet"  resultType="com.health.entity.Diet">
    select * from diet where id=#{id}
  </select>

</mapper>